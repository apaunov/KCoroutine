{
  "name": "KCoroutine",
  "dark": true,
  "author": "",
  "editorScheme": "/KCoroutine.xml",
  "colors": {
    "background": "#042132",
    "backgroundTabs": "#042D50",
    "defaultButton": "#044657",
    "accent": "#ff007e",
    "primaryText": "#ffffff",
    "selectionInactive": "#385564"
  },
  "ui": {
    "*": {
      "background": "background",
      "inactiveBackground": "backgroundTabs",

      "selectionBackground": "defaultButton",
      "selectionInactiveBackground": "selectionInactive"
    },
    "Button.background": "background",
    "FileColor": {
      "Yellow": "backgroundTabs"
    },
    "ComboBox": {
      "background": "background",
      "selectionBackground": "defaultButton",
      "nonEditableBackground": "defaultButton",
      "ArrowButton.nonEditableBackground": "defaultButton"
    },
    "Button": {
      "startBackground": "defaultButton",
      "endBackground": "defaultButton",
      "default": {
        "foreground": "primaryText",
        "startBackground": "accent",
        "endBackground": "accent"
      }
    },
    "Popup": {
      "paintBorder": true,
      "inactiveBorderColor": "defaultButton"
    }
  },
  "icons": {
    "ColorPalette": {
      "Checkbox.Background.Default.Dark": "#044657"
    }
  }
}