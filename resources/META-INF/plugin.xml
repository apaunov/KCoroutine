<idea-plugin>
    <id>com.github.apaunov.kcoroutine</id>
    <version>1.2.2</version>

    <vendor url="https://github.com/apaunov">apaunov</vendor>

    <name>KCoroutine</name>

    <description><![CDATA[
    A dark and misty theme in Kotlin Coroutine style.
    ]]>
    </description>

    <change-notes><![CDATA[
    Improved the theme's colours and overall UI in the IDE.
    ]]>
    </change-notes>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="191.0"/>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <themeProvider id="com.github.apaunov.kcoroutine" path="/KCoroutine.theme.json"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>

</idea-plugin>