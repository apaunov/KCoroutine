<idea-plugin>
    <id>com.github.apaunov.kcoroutine</id>
    <version>1.4.2</version>


    <name>KCoroutine</name>
    <category>UI</category>

    <vendor url="https://github.com/apaunov">apaunov</vendor>

    <idea-version since-build="191.0"/>

    <description><![CDATA[
    A dark and misty theme in Kotlin Coroutine style.
    ]]>
    </description>

    <change-notes><![CDATA[
    Improved the theme's colours and overall UI in the IDE.
    ]]>
    </change-notes>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <themeProvider id="com.github.apaunov.kcoroutine" path="/theme/KCoroutine.theme.json"/>
    </extensions>
</idea-plugin>